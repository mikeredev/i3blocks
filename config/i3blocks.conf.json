{
    "i3blocks": [
        {
            "system": [
                {
                    "monitor": "HDMI-0",
                    "audio_sink": "alsa_output.pci-0000_01_00.1.hdmi-stereo"
                }
            ],
            "gpu": [
                {
                    "gpu_type": "nvidia",
                    "gpu_temp_unit": "C"
                }
            ],
            "perf": [
                {
                    "fan_present": true,
                    "fan_type": "nvidia"
                }
            ],
            "devices": [
                {
                    "microphone": false,
                    "webcam": false,
                    "bluetooth": false
                }
            ],
            "time": [
                {
                    "day": 6,
                    "night": 20,
                    "adjust_glare": true,
                    "day_brightness": 1.0,
                    "day_gamma": 1.0,
                    "night_brightness": 1.0,
                    "night_gamma": 0.8
                }
            ],
            "formatting": [
                {
                    "color_ok": "#ffffff",
                    "color_warn": "#fabd2f",
                    "color_nok": "#fb4934",
                    "device_disabled": "#444444",
                    "device_inactive": "#666666",
                    "device_active_low": "#A9A9A9",
                    "device_active_high": "#ffffff"
                }
            ]
        }
    ]
}